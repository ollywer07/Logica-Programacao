#include <stdio.h>

int main () {
	
	int num;
	int i;
	int div=0;
	
	
	printf ("informe um numero: \n ");
	scanf ("%d", &num);
	for (i=1; i<=num; i++) {
		if (num % i == 0) { 
		div++;
    }
    }
  if (div == 2) {
   printf("O numero %d eh primo!", num); }
  else {
    printf("O numero %d nao eh primo!",num); }
    
   
  return 0;
}
