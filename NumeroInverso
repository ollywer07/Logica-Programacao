#include <stdio.h>

main (){
	
	int numero, m, c, d, u, inverso;
	
	printf ("Digite um numero: ");
	scanf ("%d", &numero);
	
	m = numero / 1000;
	c = (numero%1000)/100;
	d = ((numero%1000)%100)/10;
	u = (((numero%1000)%100)%10)/1;
	
	inverso = u*1000 + d*100 + c*10 + m*1;
	
	printf ("%d", inverso);
}
